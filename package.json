{
  "name": "angular-ssr",
  "version": "0.0.0",
  "description": "Angular server-side rendering implementation",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "bin": {
    "ng-render": "build/bin/render.js"
  },
  "scripts": {
    "postinstall": "npm run build && npm test",
    "prebuild": "rimraf build",
    "build": "tsc",
    "watch": "tsc --watch",
    "pretest": "npm run lint",
    "lint": "tslint --project tsconfig.json --type-check",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clbond/angular-ssr.git"
  },
  "author": "rangle.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/clbond/angular-ssr/issues"
  },
  "peerDependencies": {
    "@angular/common": "^2.4.6",
    "@angular/compiler": "^2.4.6",
    "@angular/compiler-cli": "^2.4.6",
    "@angular/core": "^2.4.6",
    "@angular/router": "^3.4.6",
    "reflect-metadata": "0.1",
    "zone.js": "0.7.6"
  },
  "optionalPeerDependencies": {
    "@ngrx/store": "^2.2.1"
  },
  "dependencies": {
    "@angular/compiler-cli": "^2.4.7",
    "@angular/tsc-wrapped": "^0.5.2",
    "@types/babel-core": "6.7.14",
    "@types/codependency": "0.1.1",
    "@types/jasmine": "2.5.41",
    "babel-core": "6.23.1",
    "babel-preset-es2015": "6.22.0",
    "codependency": "0.1.4",
    "commander": "2.9.0",
    "domino": "1.0.28",
    "reflect-metadata": "0.1.9",
    "rimraf": "2.5.4",
    "rxjs": "5.1.0",
    "scoped-logger": "0.0.15",
    "source-map-support": "0.4.11",
    "typescript": "2.2.0"
  },
  "devDependencies": {
    "@angular/common": "^2.4.6",
    "@angular/compiler": "^2.4.6",
    "@angular/core": "^2.4.6",
    "@angular/platform-browser": "^2.4.7",
    "@angular/router": "^3.4.6",
    "@ngrx/store": "^2.2.1",
    "babel-core": "6.23.1",
    "babel-preset-es2015": "6.22.0",
    "jasmine-core": "2.5.2",
    "jasmine-promises": "0.4.1",
    "jest": "18.1.0",
    "tslint": "4.4.2",
    "zone.js": "0.7.6"
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "modulePaths": [
      "<rootDir>/source"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/source/test-fixtures/dependencies.js",
    "modulePathIgnorePatterns": [
      "bundles(\\|/)"
    ],
    "transform": {
      "\\.(js|ts)$": "<rootDir>/build/transpile/process.js"
    },
    "transformIgnorePatterns": [],
    "testRegex": "source/.*/tests/.*\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
