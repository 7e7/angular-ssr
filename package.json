{
  "name": "angular-ssr",
  "version": "0.0.41",
  "description": "Angular server-side rendering implementation",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "engines": {
    "node": ">=6.0",
    "npm": ">=3.10"
  },
  "bin": {
    "ng-render": "bin/render.js"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "build": "tsc",
    "watch": "tsc --watch",
    "pretest": "npm run lint && npm run build",
    "lint": "tslint --project tsconfig.json --type-check",
    "test": "jest --verbose",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clbond/angular-ssr.git"
  },
  "contributors": [
    {
      "name": "Christopher Bond",
      "email": "cb@clbond.org",
      "url": "https://clbond.org/"
    },
    {
      "name": "rangle.io",
      "url": "https://rangle.io/"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/clbond/angular-ssr/issues"
  },
  "peerDependencies": {
    "@angular/common": "<5.0.0",
    "@angular/compiler": "<5.0.0",
    "@angular/compiler-cli": "<5.0.0",
    "@angular/core": "<5.0.0",
    "@angular/http": "<5.0.0",
    "@angular/platform-browser": "<5.0.0",
    "@angular/router": "<5.0.0",
    "@angular/tsc-wrapped": "<5.0.0",
    "reflect-metadata": "^0.1.10",
    "zone.js": "^0.7",
    "typescript": "~2.2.0"
  },
  "optionalPeerDependencies": {
    "@ngrx/store": "^2.2.1"
  },
  "dependencies": {
    "@types/babel-core": "6.7.14",
    "@types/codependency": "0.1.1",
    "@types/jasmine": "2.5.41",
    "@types/node-fetch": "^1.6.7",
    "@types/urijs": "^1.15.31",
    "babel-core": "6.23.1",
    "babel-register": "^6.24.0",
    "codependency": "0.1.4",
    "commander": "2.9.0",
    "domino": "https://github.com/clbond/domino/archive/1.0.31.tar.gz",
    "es2015-imports-to-commonjs-loose": "^1.0.4",
    "lru_map": "^0.3.3",
    "mock-local-storage": "^1.0.2",
    "node-fetch": "^1.6.3",
    "rimraf": "2.5.4",
    "rxjs": "5.1.0",
    "scoped-logger": "^0.0.19",
    "source-map-support": "0.4.11",
    "typescript": "~2.2.0",
    "urijs": "^1.18.9",
    "xhr2": "^0.1.4"
  },
  "devDependencies": {
    "@angular/common": "4.0.0-rc.3",
    "@angular/compiler": "4.0.0-rc.3",
    "@angular/compiler-cli": "4.0.0-rc.3",
    "@angular/core": "4.0.0-rc.3",
    "@angular/http": "4.0.0-rc.3",
    "@angular/platform-browser": "4.0.0-rc.3",
    "@angular/router": "4.0.0-rc.3",
    "@angular/tsc-wrapped": "4.0.0-rc.4",
    "jasmine-core": "2.5.2",
    "jest": "19.0.2",
    "reflect-metadata": "^0.1.10",
    "ts-jest": "^19.0.0",
    "tslint": "4.4.2",
    "zone.js": "0.8.2"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>/source",
      "<rootDir>/node_modules"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/source/test/fixtures/dependencies.js",
    "transform": {
      "\\.ts$": "ts-jest/dist/preprocessor.js"
    },
    "transformIgnorePatterns": [],
    "testRegex": "source/.*/tests/.*\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "files": [
    "bin",
    "build",
    "package.json",
    "source",
    "tsconfig.json",
    "tslint.json"
  ]
}
