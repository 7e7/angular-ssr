    "@angular/compiler": ">=4.0.0 <5.0.0 || >=4.0.0-rc.6 <5.0.0",
    "@angular/compiler-cli": ">=4.0.0 <5.0.0 || >=4.0.0-rc.6 <5.0.0",
    "@angular/core": ">=4.0.0 <5.0.0 || >=4.0.0-rc.6 <5.0.0",
    "@angular/forms": ">=4.0.0 <5.0.0 || >=4.0.0-rc.6 <5.0.0",
    "@angular/http": ">=4.0.0 <5.0.0 || >=4.0.0-rc.6 <5.0.0",
    "@angular/material": "2.0.0-beta.2",
    "@angular/platform-browser": ">=4.0.0 <5.0.0 || >=4.0.0-rc.6 <5.0.0",
    "@angular/platform-browser-dynamic": ">=4.0.0 <5.0.0 || >=4.0.0-rc.6 <5.0.0",
    "@angular/platform-server": ">=4.0.0 <5.0.0 || >=4.0.0-rc.6 <5.0.0",
    "@angular/router": ">=4.0.0 <5.0.0 || >=4.0.0-rc.6 <5.0.0",
    "@angular/tsc-wrapped": "^0.5.2",
